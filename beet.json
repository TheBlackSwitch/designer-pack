{
    "data_pack": {
        "load": [
            "libraries/SwitchLib-V2.0.0.zip",
            "src/datapack"
        ],
        "name": "Designer Pack"
    },
    "pipeline": [
        "plugins.aliases",
        "mecha"
    ],
    "id": "designer",
    "name": "designer",
    "description": "§aDesign your builds directly in your survival world or server §ewithout switching or interference!",
    "version": "2.3.0a",
    "require": [
        "bolt",
        "bolt_expressions"
    ],
    "output": "build",
    "meta": {
        "bolt": {
            "entrypoint": ["designer:main"]
        },
        "aliases": [
            {
                "regex": "\\$version\\$",
                "replace": "v2.0",
                "comment": "The switch lib major version"
            },
            {
                "regex": "\\$patch\\$",
                "replace": "patch-0",
                "comment": "The switch lib major version"
            },
            {
                "regex": "\\$designer-version\\$",
                "replace": "2.3.0a"
            },
            {
                "regex": "\\$test\\$",
                "replace": "say hi"
            }
        ]
    }
}